{
  "name": "graphql-executor",
  "version": "0.0.8",
  "description": "A customizable GraphQL Spec compliant Executor class.",
  "license": "MIT",
  "main": "index",
  "module": "index.mjs",
  "types": "NotSupportedTSVersion.d.ts",
  "typesVersions": {
    ">=4.1.0": {
      "*": [
        "index.d.ts"
